#include<stdio.h>
#include<inttypes.h>
int main()
{
    int n=100,i=1;
    int64_t sum;
    int sum1;
    sum1=n*(n+1)*(2*n+1)/6;//平方和如（1^2+2^2+3^2+......+n^2)等于n*(n+1)*(2*n+1)/6。
    sum=(1+100)*100/2*(1+100)*100/2;
    printf("%"PRId64"\n",sum-sum1);
    return 0;
}
