#include<stdio.h>
#include<math.h>
#include<string.h>
int a[10000]={0};
int panduan1(int x)
{
    return (int)log10(x) + 1;
}
int panduan2(int x,int *a)
{
    while(x)
    {
        if(x % 10 == 0)return 0;
        if(a[x % 10 - 1])return 0;
            a[x % 10 - 1] = 1;
            x /= 10;
    }
    return 1;
}
int panduan3(int aa,int b,int c)
{
    int a[10] = {0};
    return panduan2(aa,a) && panduan2(b,a) && panduan2(c,a);
}
int main()
{
    int sum = 0;
    for(int i = 1;i <= 100;i++)
    for(int j = i + 1;;j++)
    {
        if(panduan1(i) + panduan1(j) + panduan1(i * j) < 9)
        continue;
        if(panduan1(i) + panduan1(j) + panduan1(i * j) > 9)
        break;
        if(panduan3(i,j,i * j) && !a[i*j])
        {
            a[i * j] = 1;
            sum += i * j;
        }
    }
    printf("%d\n",sum);
    return 0;
}
//一句代码求一个数的位数
//printf("%d,printf("%d",n));
//由于printf的返回值为传入参字符的个数且嵌套使用时先运行里层printf。
